{
  "name": "@auteur/function-runner-client",
  "version": "1.0.0",
  "description": "TypeScript client for Function Runner API with WebSocket support, automatic retries, and offline queue",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "dev": "tsup index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "function-runner",
    "api-client",
    "typescript",
    "websocket",
    "offline-queue",
    "retry",
    "ai",
    "generation"
  ],
  "author": "Auteur Movie Director Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/auteur-movie-director/auteur.git",
    "directory": "frontend/src/lib/api/functionRunner"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}